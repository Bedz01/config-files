"Foto Format (Exif)"
  rename b, '<dateexif yyyy-mm-dd hh.MM.ss>', p, , 8;
  foreach($item, <get selecteditemspathnames |>) {
    $base = getpathcomponent($item, "base");
    $ext = recase(getpathcomponent($item, "ext"), "lower");
    renameitem($ext, $item, 2);
  }

