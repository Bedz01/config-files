"Video → MP3 (VBR 190k)"
  foreach($Item, <get selecteditemsnames |>) {
    $base = getpathcomponent($Item, "base");
    run "ffmpeg -i ""$Item"" -q:a 2 -c:a libmp3lame ""$base.mp3""";
  }

"Video → MP3 (CBR 192k)"
  foreach($Item, <get selecteditemsnames |>) {
    $base = getpathcomponent($Item, "base");
    run "ffmpeg -i ""$Item"" -b:a 192k -c:a libmp3lame ""$base.mp3""";
  }
